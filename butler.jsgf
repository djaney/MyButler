#JSGF V1.0;

/**
 * JSGF Grammar for English counting example
 */

grammar butler;
public <command> = <hey> (<c_open>|<c_search>|<c_query_time>);
<name> = butler;
<hey> = hey <name>;
<c_open> = open (google|chrome);
<c_search> = search;
<c_query_header> = (whats|what is);
<c_query_time> = <c_query_header> the time;
